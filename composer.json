{
    "name": "dcblogdev/laravel-blade-components",
    "description": "Laravel Blade components collection",
    "keywords": [
        "dcblogdev",
        "blade-components-forms"
    ],
    "homepage": "https://github.com/dcblogdev/laravel-blade-components",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Carr",
            "email": "dave@dcblog.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Dcblogdev\\LaravelBladeComponents\\": "src",
            "Dcblogdev\\LaravelBladeComponents\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcblogdev\\LaravelBladeComponents\\LaravelBladeComponentsServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
